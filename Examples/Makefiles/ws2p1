#Example Makefile from Worksheet 2: prob. 1
CC = g++
CCFLAGS = -g -Wall
EXEC = exec
OUT = output.txt
exec: main.C prob.o
	$(CC) $(CCFLAG) -o $(EXEC) main.o prob.o
main.o: main.C
	$(CC) $(CCFLAG) -c main.C
prob.o: prob.C prob.h
	$(CC) $(CCFLAG) -c prob.C

clear:
	rm -rf *.o
run:
	./$(EXEC)
out:
	./$(EXEC) > $(OUT)
